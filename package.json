{
	"name": "n8n-nodes-leadboxer",
	"version": "0.1.0",
	"description": "n8n node for LeadBoxer API - Domain and IP lookup for lead generation",
	"license": "MIT",
	"homepage": "https://www.leadboxer.com/",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"leadboxer",
		"lead-generation",
		"ip-lookup",
		"domain-lookup",
		"geolocation"
	],
	"author": {
		"name": "LeadBoxer",
		"email": "tech@leadboxer.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/leadboxer/n8n-nodes-leadboxer.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist",
		"LICENSE",
		"README.md"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/LeadBoxerApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/leadboxer/Leadboxer.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"publishConfig": {
    "access": "public"
  }
}